{
  "name": "mk-language-lab",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "postinstall": "prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "prepare": "husky",
    "mobile:start": "npm --prefix apps/mobile run start",
    "mobile:android": "npm --prefix apps/mobile run android",
    "mobile:ios": "npm --prefix apps/mobile run ios",
    "mobile:web": "npm --prefix apps/mobile run web",
    "mobile:test:maestro": "maestro test apps/mobile/tests",
    "mobile:build:preview:android": "npx eas build --platform android --profile preview --non-interactive",
    "mobile:build:preview:ios": "npx eas build --platform ios --profile preview --non-interactive",
    "mobile:cron:smoke": "node scripts/run-reminder-cron-smoke.mjs",
    "practice-audio:sync": "tsx scripts/practice-audio-sync.ts",
    "agents:launch": "tsx scripts/launch-sub-agents.ts",
    "agents:orchestrate": "tsx scripts/orchestrate-agents.ts",
    "agents:create-issues": "tsx scripts/create-agent-issues.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.931.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@google-cloud/translate": "^9.2.1",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.23.0",
    "@tamagui/colors": "^1.138.6",
    "@tamagui/themes": "^1.137.1",
    "@tanstack/react-query": "^5.62.0",
    "@types/papaparse": "^5.5.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^0.25.1",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo-file-system": "~19.0.17",
    "expo-router": "^3.5.23",
    "googleapis": "^165.0.0",
    "lucide-react": "^0.553.0",
    "next": "16.0.1",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.0",
    "openai": "^6.7.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native-svg": "^15.2.0",
    "react-native-web": "^0.21.0",
    "resend": "^6.5.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tamagui": "^1.137.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.5",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.0"
  },
  "overrides": {
    "tar": "7.5.2"
  }
}
