    {
      "id": "reader-b1-001",
      "title": {
        "mk": "Македонските традиции за Велигден",
        "en": "Macedonian Easter Traditions"
      },
      "level": "B1",
      "category": "culture",
      "estimatedMinutes": 7,
      "wordCount": 180,
      "xpReward": 50,
      "sentences": [
        {
          "id": "s1",
          "mk": "Велигден е еден од најважните празници во Македонија.",
          "en": "Easter is one of the most important holidays in Macedonia.",
          "audio": "/audio/reader/b1-001/s1.mp3",
          "vocabulary": [
            {"word": "најважните", "meaning": "the most important", "pos": "superlative adj"},
            {"word": "празници", "meaning": "holidays", "pos": "noun"}
          ],
          "grammarNote": "Superlative: нај- + adjective + definite ending"
        },
        {
          "id": "s2",
          "mk": "Се слави според православниот календар, обично во април или мај.",
          "en": "It is celebrated according to the Orthodox calendar, usually in April or May.",
          "audio": "/audio/reader/b1-001/s2.mp3",
          "vocabulary": [
            {"word": "се слави", "meaning": "is celebrated", "pos": "passive verb"},
            {"word": "според", "meaning": "according to", "pos": "preposition"},
            {"word": "православниот", "meaning": "the Orthodox", "pos": "adjective (def)"}
          ]
        },
        {
          "id": "s3",
          "mk": "Подготовките почнуваат со Великиот Пост, четириесет дена пред Велигден.",
          "en": "Preparations begin with Lent, forty days before Easter.",
          "audio": "/audio/reader/b1-001/s3.mp3",
          "vocabulary": [
            {"word": "подготовките", "meaning": "the preparations", "pos": "noun (def)"},
            {"word": "почнуваат", "meaning": "begin", "pos": "verb"},
            {"word": "пост", "meaning": "fast/Lent", "pos": "noun"}
          ],
          "culturalNote": "Великиот Пост = Great Lent, 40-day fasting period"
        },
        {
          "id": "s4",
          "mk": "За време на постот, верниците не јадат месо, млечни производи и јајца.",
          "en": "During the fast, believers do not eat meat, dairy products, or eggs.",
          "audio": "/audio/reader/b1-001/s4.mp3",
          "vocabulary": [
            {"word": "верниците", "meaning": "the believers", "pos": "noun (def)"},
            {"word": "млечни производи", "meaning": "dairy products", "pos": "noun phrase"}
          ]
        },
        {
          "id": "s5",
          "mk": "Во Велика Сабота, семејствата се собираат за да бојадисуваат јајца.",
          "en": "On Holy Saturday, families gather to dye eggs.",
          "audio": "/audio/reader/b1-001/s5.mp3",
          "vocabulary": [
            {"word": "се собираат", "meaning": "gather", "pos": "reflexive verb"},
            {"word": "бојадисуваат", "meaning": "dye/paint", "pos": "verb"}
          ]
        },
