    {
      "id": "reader-a1-002",
      "title": {
        "mk": "Моето утро",
        "en": "My Morning"
      },
      "level": "A1",
      "category": "daily-life",
      "estimatedMinutes": 3,
      "wordCount": 90,
      "xpReward": 20,
      "grammarHighlight": {
        "topic": "Present tense verbs for daily routine",
        "topicMk": "Сегашно време за секојдневни активности",
        "examples": ["станувам", "јадам", "пијам", "одам"]
      },
      "sentences": [
        {
          "id": "s1",
          "mk": "Секое утро станувам во седум часот.",
          "en": "Every morning I wake up at seven o'clock.",
          "audio": "/audio/reader/a1-002/s1.mp3",
          "vocabulary": [
            {"word": "секое", "meaning": "every", "pos": "adjective"},
            {"word": "утро", "meaning": "morning", "pos": "noun"},
            {"word": "станувам", "meaning": "I wake up/get up", "pos": "verb"}
          ],
          "grammarNote": "станувам - present tense, 1st person singular"
        },
        {
          "id": "s2",
          "mk": "Јас одам во бањата и се мијам.",
          "en": "I go to the bathroom and wash myself.",
          "audio": "/audio/reader/a1-002/s2.mp3",
          "vocabulary": [
            {"word": "бањата", "meaning": "the bathroom", "pos": "noun (def)"},
            {"word": "се мијам", "meaning": "I wash myself", "pos": "reflexive verb"}
          ]
        },
        {
          "id": "s3",
          "mk": "Си ги мијам забите.",
          "en": "I brush my teeth.",
          "audio": "/audio/reader/a1-002/s3.mp3",
          "vocabulary": [
            {"word": "заби", "meaning": "teeth", "pos": "noun"},
            {"word": "си ги мијам", "meaning": "I wash/brush (my)", "pos": "verb phrase"}
          ]
        },
        {
          "id": "s4",
          "mk": "Потоа се облекувам.",
          "en": "Then I get dressed.",
          "audio": "/audio/reader/a1-002/s4.mp3",
          "vocabulary": [
            {"word": "потоа", "meaning": "then", "pos": "adverb"},
            {"word": "се облекувам", "meaning": "I get dressed", "pos": "reflexive verb"}
          ]
        },
        {
          "id": "s5",
          "mk": "Јас одам во кујната за појадок.",
          "en": "I go to the kitchen for breakfast.",
          "audio": "/audio/reader/a1-002/s5.mp3",
          "vocabulary": [
            {"word": "кујната", "meaning": "the kitchen", "pos": "noun (def)"},
            {"word": "појадок", "meaning": "breakfast", "pos": "noun"}
          ]
        },
        {
          "id": "s6",
          "mk": "За појадок јадам леб со мед.",
          "en": "For breakfast I eat bread with honey.",
          "audio": "/audio/reader/a1-002/s6.mp3",
          "vocabulary": [
            {"word": "јадам", "meaning": "I eat", "pos": "verb"},
            {"word": "леб", "meaning": "bread", "pos": "noun"},
            {"word": "мед", "meaning": "honey", "pos": "noun"}
          ]
        },
        {
          "id": "s7",
          "mk": "Пијам чај или сок.",
          "en": "I drink tea or juice.",
          "audio": "/audio/reader/a1-002/s7.mp3",
          "vocabulary": [
            {"word": "пијам", "meaning": "I drink", "pos": "verb"},
            {"word": "чај", "meaning": "tea", "pos": "noun"},
            {"word": "сок", "meaning": "juice", "pos": "noun"}
          ]
        },
        {
          "id": "s8",
          "mk": "Мајка ми прави кафе секое утро.",
          "en": "My mother makes coffee every morning.",
          "audio": "/audio/reader/a1-002/s8.mp3",
          "vocabulary": [
            {"word": "прави", "meaning": "makes", "pos": "verb"},
            {"word": "кафе", "meaning": "coffee", "pos": "noun"}
          ]
        },
        {
          "id": "s9",
          "mk": "Во осум часот сум готов.",
          "en": "At eight o'clock I am ready.",
          "audio": "/audio/reader/a1-002/s9.mp3",
          "vocabulary": [
            {"word": "осум", "meaning": "eight", "pos": "number"},
            {"word": "готов", "meaning": "ready", "pos": "adjective"}
          ]
        },
        {
          "id": "s10",
          "mk": "Ја земам мојата торба.",
          "en": "I take my bag.",
          "audio": "/audio/reader/a1-002/s10.mp3",
          "vocabulary": [
            {"word": "земам", "meaning": "I take", "pos": "verb"},
            {"word": "торба", "meaning": "bag", "pos": "noun"}
          ]
        },
        {
          "id": "s11",
          "mk": "Се поздравувам со семејството.",
          "en": "I say goodbye to my family.",
          "audio": "/audio/reader/a1-002/s11.mp3",
          "vocabulary": [
            {"word": "се поздравувам", "meaning": "I say goodbye", "pos": "reflexive verb"},
            {"word": "семејството", "meaning": "the family", "pos": "noun (def)"}
          ]
        },
        {
          "id": "s12",
          "mk": "Одам на работа со автобус.",
          "en": "I go to work by bus.",
          "audio": "/audio/reader/a1-002/s12.mp3",
          "vocabulary": [
            {"word": "работа", "meaning": "work", "pos": "noun"},
            {"word": "автобус", "meaning": "bus", "pos": "noun"}
          ]
        }
      ],
      "comprehensionQuestions": [
        {
          "question": {
            "mk": "Во колку часот станува?",
            "en": "What time does he/she wake up?"
          },
          "options": [
            {"mk": "Шест", "correct": false},
            {"mk": "Седум", "correct": true},
            {"mk": "Осум", "correct": false}
          ]
        },
        {
          "question": {
            "mk": "Што јаде за појадок?",
            "en": "What does he/she eat for breakfast?"
          },
          "options": [
            {"mk": "Јајца", "correct": false},
            {"mk": "Леб со мед", "correct": true},
            {"mk": "Овошје", "correct": false}
          ]
        },
        {
          "question": {
            "mk": "Како оди на работа?",
            "en": "How does he/she go to work?"
          },
          "options": [
            {"mk": "Со автомобил", "correct": false},
            {"mk": "Пешки", "correct": false},
            {"mk": "Со автобус", "correct": true}
          ]
        }
      ]
    },
    {
      "id": "reader-a2-001",
      "title": {
        "mk": "Еден ден во Охрид",
        "en": "A Day in Ohrid"
      },
      "level": "A2",
      "category": "travel",
      "estimatedMinutes": 5,
      "wordCount": 130,
      "xpReward": 35,
      "sentences": [
        {
          "id": "s1",
          "mk": "Минатата сабота отидов во Охрид со мојата пријателка Јана.",
          "en": "Last Saturday I went to Ohrid with my friend Jana.",
          "audio": "/audio/reader/a2-001/s1.mp3",
          "vocabulary": [
            {"word": "минатата", "meaning": "last (fem.)", "pos": "adjective"},
            {"word": "сабота", "meaning": "Saturday", "pos": "noun"},
            {"word": "отидов", "meaning": "I went", "pos": "verb (past)"}
          ],
          "grammarNote": "Past tense: отидов (perfective of одам)"
        },
        {
          "id": "s2",
          "mk": "Тргнавме рано наутро со автобус.",
          "en": "We left early in the morning by bus.",
          "audio": "/audio/reader/a2-001/s2.mp3",
          "vocabulary": [
            {"word": "тргнавме", "meaning": "we left", "pos": "verb (past)"},
            {"word": "рано", "meaning": "early", "pos": "adverb"},
            {"word": "наутро", "meaning": "in the morning", "pos": "adverb"}
          ]
        },
        {
          "id": "s3",
          "mk": "Патувањето траеше околу три часа.",
          "en": "The journey lasted about three hours.",
          "audio": "/audio/reader/a2-001/s3.mp3",
          "vocabulary": [
            {"word": "патувањето", "meaning": "the journey", "pos": "noun"},
            {"word": "траеше", "meaning": "lasted", "pos": "verb (past)"},
            {"word": "околу", "meaning": "about/around", "pos": "preposition"}
          ]
        },
        {
          "id": "s4",
          "mk": "Кога пристигнавме, времето беше прекрасно.",
          "en": "When we arrived, the weather was wonderful.",
          "audio": "/audio/reader/a2-001/s4.mp3",
          "vocabulary": [
            {"word": "кога", "meaning": "when", "pos": "conjunction"},
            {"word": "пристигнавме", "meaning": "we arrived", "pos": "verb (past)"},
            {"word": "прекрасно", "meaning": "wonderful", "pos": "adjective"}
          ]
        },
        {
          "id": "s5",
          "mk": "Прво отидовме да го видиме Охридското Езеро.",
          "en": "First we went to see Lake Ohrid.",
          "audio": "/audio/reader/a2-001/s5.mp3",
          "vocabulary": [
            {"word": "прво", "meaning": "first", "pos": "adverb"},
            {"word": "езеро", "meaning": "lake", "pos": "noun"}
          ],
          "grammarNote": "Охридското - definite adjective form"
        },
        {
          "id": "s6",
          "mk": "Водата беше чиста и сина.",
          "en": "The water was clean and blue.",
          "audio": "/audio/reader/a2-001/s6.mp3",
          "vocabulary": [
            {"word": "вода", "meaning": "water", "pos": "noun"},
            {"word": "чиста", "meaning": "clean", "pos": "adjective"},
            {"word": "сина", "meaning": "blue", "pos": "adjective"}
          ]
        },
        {
          "id": "s7",
          "mk": "Потоа прошетавме низ стариот дел од градот.",
          "en": "Then we walked through the old part of the city.",
          "audio": "/audio/reader/a2-001/s7.mp3",
          "vocabulary": [
            {"word": "потоа", "meaning": "then/after", "pos": "adverb"},
            {"word": "прошетавме", "meaning": "we walked", "pos": "verb (past)"},
            {"word": "стариот", "meaning": "the old", "pos": "adjective (def)"}
          ]
        },
        {
          "id": "s8",
          "mk": "Ја посетивме црквата Свети Јован Канео.",
          "en": "We visited the Church of St. John at Kaneo.",
          "audio": "/audio/reader/a2-001/s8.mp3",
          "vocabulary": [
            {"word": "посетивме", "meaning": "we visited", "pos": "verb (past)"},
            {"word": "црквата", "meaning": "the church", "pos": "noun (def)"}
          ],
          "culturalNote": "Iconic 13th-century church overlooking the lake"
        },
        {
          "id": "s9",
          "mk": "Гледката оттаму беше неверојатна.",
          "en": "The view from there was incredible.",
          "audio": "/audio/reader/a2-001/s9.mp3",
          "vocabulary": [
            {"word": "гледката", "meaning": "the view", "pos": "noun (def)"},
            {"word": "оттаму", "meaning": "from there", "pos": "adverb"},
            {"word": "неверојатна", "meaning": "incredible", "pos": "adjective"}
          ]
        },
        {
          "id": "s10",
          "mk": "За ручек јадевме охридска пастрмка.",
          "en": "For lunch we ate Ohrid trout.",
          "audio": "/audio/reader/a2-001/s10.mp3",
          "vocabulary": [
            {"word": "ручек", "meaning": "lunch", "pos": "noun"},
            {"word": "јадевме", "meaning": "we ate", "pos": "verb (past)"},
            {"word": "пастрмка", "meaning": "trout", "pos": "noun"}
          ],
          "culturalNote": "Ohrid trout is a famous local specialty"
        },
        {
          "id": "s11",
          "mk": "Рибата беше многу вкусна!",
          "en": "The fish was very delicious!",
          "audio": "/audio/reader/a2-001/s11.mp3",
          "vocabulary": [
            {"word": "рибата", "meaning": "the fish", "pos": "noun (def)"},
            {"word": "вкусна", "meaning": "delicious", "pos": "adjective"}
          ]
        },
        {
          "id": "s12",
          "mk": "Навечер се вративме дома, уморни но среќни.",
          "en": "In the evening we returned home, tired but happy.",
          "audio": "/audio/reader/a2-001/s12.mp3",
          "vocabulary": [
            {"word": "навечер", "meaning": "in the evening", "pos": "adverb"},
            {"word": "се вративме", "meaning": "we returned", "pos": "verb (past)"},
            {"word": "уморни", "meaning": "tired", "pos": "adjective"}
          ]
        },
        {
          "id": "s13",
          "mk": "Сакам да се вратам повторно!",
          "en": "I want to go back again!",
          "audio": "/audio/reader/a2-001/s13.mp3",
          "vocabulary": [
            {"word": "повторно", "meaning": "again", "pos": "adverb"}
          ]
        }
      ],
      "comprehensionQuestions": [
        {
          "question": {
            "mk": "Колку време траеше патувањето?",
            "en": "How long did the journey take?"
          },
          "options": [
            {"mk": "Два часа", "correct": false},
            {"mk": "Три часа", "correct": true},
            {"mk": "Четири часа", "correct": false}
          ]
        },
        {
          "question": {
            "mk": "Што јадеа за ручек?",
