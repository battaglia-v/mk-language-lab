      "id": "reader-a1-001",
      "title": {
        "mk": "Семејството на Ана",
        "en": "Ana's Family"
      },
      "level": "A1",
      "category": "family",
      "estimatedMinutes": 3,
      "wordCount": 85,
      "xpReward": 20,
      "sentences": [
        {
          "id": "s1",
          "mk": "Јас се викам Ана.",
          "en": "My name is Ana.",
          "audio": "/audio/reader/a1-001/s1.mp3",
          "vocabulary": [
            {"word": "се викам", "meaning": "my name is", "pos": "verb phrase"}
          ]
        },
        {
          "id": "s2",
          "mk": "Имам дваесет години.",
          "en": "I am twenty years old.",
          "audio": "/audio/reader/a1-001/s2.mp3",
          "vocabulary": [
            {"word": "дваесет", "meaning": "twenty", "pos": "number"},
            {"word": "години", "meaning": "years", "pos": "noun"}
          ]
        },
        {
          "id": "s3",
          "mk": "Живеам во Скопје со моето семејство.",
          "en": "I live in Skopje with my family.",
          "audio": "/audio/reader/a1-001/s3.mp3",
          "vocabulary": [
            {"word": "живеам", "meaning": "I live", "pos": "verb"},
            {"word": "семејство", "meaning": "family", "pos": "noun"}
          ]
        },
        {
          "id": "s4",
          "mk": "Моите родители се Марко и Лена.",
          "en": "My parents are Marko and Lena.",
          "audio": "/audio/reader/a1-001/s4.mp3",
          "vocabulary": [
            {"word": "родители", "meaning": "parents", "pos": "noun"},
            {"word": "моите", "meaning": "my (plural)", "pos": "possessive"}
          ]
        },
        {
          "id": "s5",
          "mk": "Татко ми е доктор.",
          "en": "My father is a doctor.",
          "audio": "/audio/reader/a1-001/s5.mp3",
          "vocabulary": [
            {"word": "татко", "meaning": "father", "pos": "noun"},
            {"word": "доктор", "meaning": "doctor", "pos": "noun"}
          ]
        },
        {
          "id": "s6",
          "mk": "Мајка ми е учителка.",
          "en": "My mother is a teacher.",
          "audio": "/audio/reader/a1-001/s6.mp3",
          "vocabulary": [
            {"word": "мајка", "meaning": "mother", "pos": "noun"},
            {"word": "учителка", "meaning": "teacher (f)", "pos": "noun"}
          ]
        },
        {
          "id": "s7",
          "mk": "Имам еден брат и една сестра.",
          "en": "I have one brother and one sister.",
          "audio": "/audio/reader/a1-001/s7.mp3",
          "vocabulary": [
            {"word": "брат", "meaning": "brother", "pos": "noun"},
            {"word": "сестра", "meaning": "sister", "pos": "noun"}
          ]
        },
        {
          "id": "s8",
          "mk": "Мојот брат се вика Петар.",
          "en": "My brother's name is Petar.",
          "audio": "/audio/reader/a1-001/s8.mp3",
          "vocabulary": [
            {"word": "мојот", "meaning": "my (masc.)", "pos": "possessive"}
          ]
        },
        {
          "id": "s9",
          "mk": "Тој има осумнаесет години.",
          "en": "He is eighteen years old.",
          "audio": "/audio/reader/a1-001/s9.mp3",
          "vocabulary": [
            {"word": "осумнаесет", "meaning": "eighteen", "pos": "number"}
          ]
        },
        {
          "id": "s10",
          "mk": "Мојата сестра се вика Марија.",
          "en": "My sister's name is Marija.",
          "audio": "/audio/reader/a1-001/s10.mp3",
          "vocabulary": [
            {"word": "мојата", "meaning": "my (fem.)", "pos": "possessive"}
          ]
        },
        {
          "id": "s11",
          "mk": "Таа има петнаесет години.",
          "en": "She is fifteen years old.",
          "audio": "/audio/reader/a1-001/s11.mp3",
          "vocabulary": [
            {"word": "петнаесет", "meaning": "fifteen", "pos": "number"}
          ]
        },
        {
          "id": "s12",
          "mk": "Ние сме среќно семејство.",
          "en": "We are a happy family.",
          "audio": "/audio/reader/a1-001/s12.mp3",
          "vocabulary": [
            {"word": "среќно", "meaning": "happy", "pos": "adjective"}
          ]
        }
      ],
      "comprehensionQuestions": [
        {
          "question": {
