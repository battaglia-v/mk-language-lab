  "conjugationPatterns": {
    "regular-a": {
      "description": "Regular -а conjugation (most common)",
      "endings": {
        "present": ["-ам", "-аш", "-а", "-аме", "-ате", "-аат"],
        "past": ["-ав", "-аше", "-аше", "-авме", "-авте", "-аа"]
      }
    },
    "regular-i": {
      "description": "Regular -и conjugation",
      "endings": {
        "present": ["-ам", "-иш", "-и", "-име", "-ите", "-ат"],
        "past": ["-ев", "-еше", "-еше", "-евме", "-евте", "-еа"]
      }
    },
    "regular-e": {
      "description": "Regular -е conjugation",
      "endings": {
        "present": ["-ам", "-еш", "-е", "-еме", "-ете", "-ат"],
        "past": ["-ев", "-еше", "-еше", "-евме", "-евте", "-еа"]
      }
    },
    "regular-uva": {
      "description": "Regular -ува conjugation",
      "endings": {
        "present": ["-увам", "-уваш", "-ува", "-уваме", "-увате", "-уваат"],
        "past": ["-ував", "-уваше", "-уваше", "-увавме", "-увавте", "-уваа"]
      }
    }
  },
