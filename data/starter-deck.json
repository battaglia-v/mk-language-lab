    {
      "id": "sd-037",
      "mk": "кој",
      "en": "who",
      "category": "questions",
      "audio": "/audio/koj.mp3",
      "example": {
        "mk": "Кој е тоа?",
        "en": "Who is that?"
      },
      "notes": "Masculine form; која (f), кое (n)"
