      "notes": "First person singular pronoun"
    },
    {
      "id": "sd-018",
      "mk": "ти",
      "en": "you (informal)",
      "category": "pronouns",
      "audio": "/audio/ti.mp3",
      "example": {
        "mk": "Ти си добар пријател.",
        "en": "You are a good friend."
      },
      "notes": "Singular informal 'you'"
    },
    {
      "id": "sd-019",
      "mk": "тој",
      "en": "he",
      "category": "pronouns",
      "audio": "/audio/toj.mp3",
      "example": {
        "mk": "Тој е мој брат.",
        "en": "He is my brother."
      },
      "notes": "Third person masculine singular"
    },
    {
      "id": "sd-020",
      "mk": "таа",
      "en": "she",
      "category": "pronouns",
      "audio": "/audio/taa.mp3",
      "example": {
        "mk": "Таа е моја сестра.",
        "en": "She is my sister."
      },
      "notes": "Third person feminine singular"
    },
    {
      "id": "sd-021",
      "mk": "ние",
      "en": "we",
      "category": "pronouns",
      "audio": "/audio/nie.mp3",
      "example": {
        "mk": "Ние учиме македонски.",
        "en": "We are learning Macedonian."
      },
      "notes": "First person plural"
    },
    {
      "id": "sd-022",
      "mk": "вие",
      "en": "you (plural/formal)",
      "category": "pronouns",
      "audio": "/audio/vie.mp3",
      "example": {
        "mk": "Вие сте добредојдени.",
        "en": "You are welcome."
      },
      "notes": "Plural or formal singular 'you'"
    },
    {
      "id": "sd-023",
      "mk": "тие",
      "en": "they",
      "category": "pronouns",
      "audio": "/audio/tie.mp3",
      "example": {
        "mk": "Тие живеат во Скопје.",
        "en": "They live in Skopje."
      },
      "notes": "Third person plural (all genders)"
    },
