  "items": [
    {
      "id": "sd-001",
      "mk": "здраво",
      "en": "hello",
      "category": "greetings",
      "audio": "/audio/zdravo.mp3",
      "example": {
        "mk": "Здраво, како си?",
        "en": "Hello, how are you?"
      },
      "notes": "Informal greeting, used with friends and family"
    },
    {
      "id": "sd-002",
      "mk": "добар ден",
      "en": "good day",
      "category": "greetings",
      "audio": "/audio/dobar-den.mp3",
      "example": {
        "mk": "Добар ден, господине.",
        "en": "Good day, sir."
      },
      "notes": "Formal greeting, used during daytime"
    },
    {
      "id": "sd-003",
      "mk": "добро утро",
      "en": "good morning",
      "category": "greetings",
      "audio": "/audio/dobro-utro.mp3",
      "example": {
        "mk": "Добро утро! Како спа?",
        "en": "Good morning! How did you sleep?"
      },
      "notes": "Used until around noon"
    },
    {
      "id": "sd-004",
      "mk": "добра вечер",
      "en": "good evening",
      "category": "greetings",
      "audio": "/audio/dobra-vecer.mp3",
      "example": {
        "mk": "Добра вечер на сите!",
        "en": "Good evening to everyone!"
      },
      "notes": "Used after sunset"
    },
    {
      "id": "sd-005",
      "mk": "лека ноќ",
      "en": "good night",
      "category": "greetings",
      "audio": "/audio/leka-nokj.mp3",
      "example": {
        "mk": "Лека ноќ, сони убаво.",
        "en": "Good night, sleep well."
      },
      "notes": "Said when going to bed"
    },
    {
      "id": "sd-006",
      "mk": "довидување",
      "en": "goodbye",
      "category": "greetings",
      "audio": "/audio/doviduvanje.mp3",
      "example": {
        "mk": "Довидување, ќе се видиме утре.",
        "en": "Goodbye, see you tomorrow."
      },
      "notes": "Formal farewell"
    },
    {
      "id": "sd-007",
      "mk": "ќао",
      "en": "bye (informal)",
      "category": "greetings",
      "audio": "/audio/kjao.mp3",
      "example": {
        "mk": "Ќао, се чуеме!",
        "en": "Bye, talk to you later!"
      },
      "notes": "Informal, from Italian 'ciao'"
    },
    {
